<mvc:View controllerName="invoice.demo.controller.View2"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m" xmlns:u="sap.ui.unified" xmlns:l="sap.ui.layout" xmlns:core="sap.ui.core" xmlns:ui="sap.ui.table" xmlns:f="sap.ui.layout.form">
     <Shell id="shell">
		<App id="app" class="myAppDemoWT">
			<pages>
				<Page id="page" titleAlignment="Center">
					<customHeader>
						<Bar id="_IDGenBar1">
							<contentLeft>
                             <Button id="_IDGenButton0"
      
        type="Emphasized"
        icon="sap-icon://navigation-left-arrow"
        press="onNavToSecondView1" />
								<Image id="_IDGenImage1" src="{logoModel>/logoSrc}" alt="Logo" height="{logoModel>/height}" width="{logoModel>/width}"/>
							</contentLeft>
							<contentMiddle>
								<FlexBox id="_IDGenFlexBox1" alignItems="Center" justifyContent="Center">
									<items>
										<Text id="_IDGenText1" text="ADMINISTRATOR PORTAL" textAlign="Center" renderWhitespace="true" class="redText"/>
									</items>
								</FlexBox>
							</contentMiddle>
						</Bar>
					</customHeader>
					<content>
						<IconTabBar id="idIconTabBarFiori21" expandable="false" expanded="{device>/isNoPhone}" class="sapUiResponsiveContentPadding" >
							<items>
								<IconTabFilter id="_IDGenIconTabFilter1" text="EXTRACTED RECORDS" key="approverDelegation">
						
  <Table id="idProductsTable"
		inset="false"
		items="{
			path: 'successIdModel>/data'
		}">
		<columns>
			<Column id="_IDGenColumn5"
				width="8em">
				<Text id="_IDGenText6" text="Employee Name" />
			</Column>
            <Column id="_IDGenColumn7"
				width="8em">
				<Text id="_IDGenText8" text="Item ID" />
			</Column>
            <Column id="_IDGenColumn9"
				width="8em">
				<Text id="_IDGenText13" text="Item Description" />
			</Column>
			<Column id="_IDGenColumn10"
				width="8em">
				<Text id="_IDGenText15" text="Department" />
			</Column>
            <Column id="_IDGenColumn12"
				width="8em">
				<Text id="_IDGenText14" text="Date" />
			</Column>
			<Column id="_IDGenColumn14"
				width="8em">
				<Text id="_IDGenText100" text="Document Number" />
			</Column>
			<Column id="_IDGenColumn90"
				width="8em">
				<Text id="_IDGenText101" text="Gross Amount" />
			</Column>
			<Column id="_IDGenColumn97"
				width="8em">
				<Text id="_IDGenText10" text="Company Name" />
			</Column>
			<Column id="_IDGenColumn23"
				width="8em">
				<Text id="_IDGenText24" text="Action" />
			</Column>
			<Column id="_IDGenColumn21"
				width="8em">
				<Text id="_IDGenText22" text="Status" />
			</Column>
			<Column id="_IDGenColumn19"
				width="8em">
				<Text id="_IDGenText20" text="Preview" />
			</Column>
		</columns>
		<items>
			<ColumnListItem id="_IDGenColumnListItem2" vAlign="Middle">
				<cells>
			
					<Text id="_IDGenText70"
						text="{successIdModel>employeeName}" />
                        <Text id="_IDGenText110"
						text="{successIdModel>itemId}" />
						<Text id="_IDGenText112"
						text="{successIdModel>itemDescription}" />
						<Text id="_IDGenText111"
						text="{successIdModel>department}" />
                        <Text id="_IDGenText120"
						text="{successIdModel>currentDate}" />
				       	<Text id="_IDGenText7"
						text="{successIdModel>DocumentNumber}" />
                        <Text id="_IDGenText11"
						text="{successIdModel>GrossAmount}" />
                        <Text id="_IDGenText12"
						text="{successIdModel>SenderName}" />
						 <Text id="_IDGenText25"
						text="{successIdModel>action}" />
						 <Text id="_IDGenText19"
						text="{successIdModel>status}" />
						<Button id="_IDGenButtonPreview" text="Preview" type="Emphasized" press="onPreviewFileInProgress" icon="sap-icon://document-text" />
				</cells>
			</ColumnListItem>
		</items>
	</Table>
  

								</IconTabFilter>
								
							
							</items>
						</IconTabBar>
						
					</content>
				</Page>
			</pages>
		</App>
	</Shell>
    
    </mvc:View>