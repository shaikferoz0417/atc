<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form"
	xmlns:core="sap.ui.core">
	<VBox class="sapUiSmallMargin">
	
		<f:SimpleForm id="SimpleFormDisplayColumn_twoGroups234"
			editable="false"
			layout="ColumnLayout"
			title="Supplier"
			columnsM="2"
			columnsL="3"
			columnsXL="4"
			>
			<f:content>
				<core:Title text="Address" />
				<Label text="Name" />
				<Text id="nameText" text="{/SupplierCollection/0/SupplierName}" />
				<Label text="Street/No." />
				<Text text="{/SupplierCollection/0/Street} {/SupplierCollection/0/HouseNumber}" />
				<Label text="ZIP Code/City" />
				<Text text="{/SupplierCollection/0/ZIPCode} {/SupplierCollection/0/City}" />
				<Label text="Country" />
				<Text id="countryText" text="{/SupplierCollection/0/Country}" />
				<Label text="Web" />
				<Link text="Url" href="{/SupplierCollection/0/Url}" />
				<core:Title text="Contact" />
				<Label text="Twitter" />
				<Text text="{/SupplierCollection/0/Twitter}" />
				<Label text="Email" />
				<Text text="{/SupplierCollection/0/Email}" />
				<Label text="Tel." />
				<Text text="{/SupplierCollection/0/Tel}" />
				<Label text="SMS" />
				<Text text="{/SupplierCollection/0/Sms}" />
				<Label text="Mobile" />
				<Text text="{/SupplierCollection/0/Mobile}" />
				<Label text="Pager" />
				<Text text="{/SupplierCollection/0/Pager}" />
				<Label text="Fax" />
				<Text text="{/SupplierCollection/0/Fax}" />
			</f:content>
		</f:SimpleForm>
	</VBox>
</core:FragmentDefinition>