{
    "welcomeFile": "/index.html",
    "authenticationMethod": "route",
    "routes": [
      {
        "source": "^/resources/(.*)$",
        "target": "/resources/$1",
        "authenticationType": "none",
        "destination": "ui5"
      },
      {
        "source": "^/test-resources/(.*)$",
        "target": "/test-resources/$1",
        "authenticationType": "none",
        "destination": "ui5"
      },
      {
        "source": "^/api/(.*)$",
        "target": "/public/workflow/rest/v1/workflow-instances/$1",
        "destination": "spa_api_gateway",
        "csrfProtection": false
      }
    ]
  }
  