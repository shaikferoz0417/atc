<core:FragmentDefinition xmlns:form="sap.ui.layout.form" xmlns:core="sap.ui.core" xmlns:u="sap.ui.unified" xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m">
	<Table id="complaintTable" items="{/ComplaintList/data}">
		<columns>
			<Column>
				<Text text="Complaint ID"/>
			</Column>
			<Column>
				<Text text="Invoice Number"/>
			</Column>
			<Column>
				<Text text="Customer"/>
			</Column>
			<Column>
				<Text text="Complaint Reason"/>
			</Column>
			<Column>
				<Text text="Status"/>
			</Column>
		</columns>
		<items>
			<ColumnListItem>
				<cells>
					<Text text="{ComplaintID}"/>
					<Text text="{InvoiceNumber}"/>
					<Text text="{Customer}"/>
					<Text text="{ComplaintReason}"/>
					<Text text="{Status}"/>
				</cells>
			</ColumnListItem>
		</items>
	</Table>
</core:FragmentDefinition>